SRCVERS[0]=1.0.4
SRCPACK[0]=${XORGIREPO}/lib/${SRCNAME[0]}-${SRCVERS[0]}.tar.bz2

build0()
{
configure
make ${JOBS}
make install DESTDIR="${PKG}"
changelog ChangeLog
license COPYING
rm -f ${PKG}/usr/lib$(libdirsuffix)/*.la
}

